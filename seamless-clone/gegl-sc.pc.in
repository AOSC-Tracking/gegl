prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@/sc

Name: GEGL Seamless Cloning
Description: Seamless Cloning Library Based on GEGL
Version: @GEGL_REAL_VERSION@
