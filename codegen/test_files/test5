GENERIC_IMAGE_DECL_BEGIN
Pixel ca(color,alpha);
Pixel c(color);
Channel b;
int i;
float f; 
GENERIC_IMAGE_DECL_END

GENERIC_IMAGE_BLOCK_BEGIN
ca = ca * ca;
ca = b * ca;
ca = i * ca;
ca = f * ca;
ca = (f * ca) * ca;

ca = ca / ca;
ca = b / ca;
ca = i / ca;
ca = f / ca;
ca = (f * ca) / ca;
GENERIC_IMAGE_BLOCK_END
