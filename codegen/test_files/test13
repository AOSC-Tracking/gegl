GENERIC_IMAGE_DECL_BEGIN
VectorChan ca(color,alpha);
VectorChan c(color);
Chan b;
int i;
float f; 
GENERIC_IMAGE_DECL_END

GENERIC_IMAGE_BLOCK_BEGIN
ca = ca * ca;
ca = b * ca;
ca = i * ca;
ca = f * ca;
ca = (f * ca) * ca;

ca = ca / ca;
ca = b / ca;
ca = i / ca;
ca = f / ca;
ca = (f * ca) / ca;

GENERIC_IMAGE_BLOCK_END

