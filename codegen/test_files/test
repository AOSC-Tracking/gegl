/* For testing comments */

CODEGEN_DECL_BEGIN
gint i;
gfloat f;
Chan a; 
VectorChan d(color,alpha);
VectorChan s(color,alpha);
VectorChan s1(color,alpha), s2(color,alpha);
VectorChan s3(color,alpha); /*a comment*/
CODEGEN_DECL_END

CODEGEN_IMAGE_DATA_INIT


/* d = s1 * s2; */
CODEGEN_BLOCK_BEGIN
d = s1 * s2;
CODEGEN_BLOCK_END


/* d = CHAN_CLAMP(s1 + s2); */
CODEGEN_BLOCK_BEGIN
d = CHAN_CLAMP(s1 + s2);
CODEGEN_BLOCK_END


/* d = CHAN_CLAMP (5 *s); */
CODEGEN_BLOCK_BEGIN
d = CHAN_CLAMP (5 *s);
CODEGEN_BLOCK_END

/* d = (s1 + s2 + s3)/3; */
CODEGEN_BLOCK_BEGIN
d = (s1 + s2 + s3)/3;
CODEGEN_BLOCK_END


/* d = s * s1 + (WP-s) * s2; */
CODEGEN_BLOCK_BEGIN
d = s * s1 + (WP-s) * s2;
CODEGEN_BLOCK_END


/*d = .333 * (s1 + s2 + s3);*/ 
CODEGEN_BLOCK_BEGIN
d = .333 * (s1 + s2 + s3); 
CODEGEN_BLOCK_END


/* d = CHAN_CLAMP(s1 * s2 + s3); */
CODEGEN_BLOCK_BEGIN
d = CHAN_CLAMP(s1 * s2 + s3);
CODEGEN_BLOCK_END


/* d = WP - ((WP - s1) * (WP - s2)); */
CODEGEN_BLOCK_BEGIN
d = WP - ((WP - s1) * (WP - s2));
CODEGEN_BLOCK_END


/* d = MAX(s1, s2); */
CODEGEN_BLOCK_BEGIN
d = MAX(s1, s2);
CODEGEN_BLOCK_END


/* d = CHAN_CLAMP(s1 - s2) */;
CODEGEN_BLOCK_BEGIN
d = CHAN_CLAMP(s1 - s2);
CODEGEN_BLOCK_END


/* d = ABS(s1 - s2); */
CODEGEN_BLOCK_BEGIN
d = ABS(s1 - s2);
CODEGEN_BLOCK_END


/* d = MIN(s1, s2); */
CODEGEN_BLOCK_BEGIN
d = MIN(s1, s2);
CODEGEN_BLOCK_END


/*
d_color = CHAN_CLAMP(s1_color + s2_color);
d_alpha = s1_alpha;
*/
CODEGEN_BLOCK_BEGIN
d_color = CHAN_CLAMP(s1_color + s2_color);
d_alpha = WP;
CODEGEN_BLOCK_END


/* d_color = d_alpha * d_color; */
CODEGEN_BLOCK_BEGIN
d_color = d_alpha * d_color;
CODEGEN_BLOCK_END

/* d_color = d_color / d_alpha; */
CODEGEN_BLOCK_BEGIN
d_color = d_color / d_alpha;
CODEGEN_BLOCK_END

/*
d = d / 5.0;
d = d_color / 5;
d_color / a;
d = d / a;
*/
CODEGEN_BLOCK_BEGIN
d = d / 5.0;
d = d_color / 5;
d_color / a;
d = d / a;
CODEGEN_BLOCK_END

/*
d_color / a;
d = d / a;
*/
CODEGEN_BLOCK_BEGIN
d_color / a;
d = d / a;
CODEGEN_BLOCK_END

/* d = d + 5.0; */
CODEGEN_BLOCK_BEGIN
d = d + 5.0;
CODEGEN_BLOCK_END

/* d = ZERO */
CODEGEN_BLOCK_BEGIN
d = ZERO;
CODEGEN_BLOCK_END
