DATATYPE_BEGIN
DATATYPE           guint16
PROMOTE_TYPE		   (guint32) 
SIGNED_PROMOTE_TYPE		   (gint32) 
WP                 65535UL
WP_NORM            (1/65535.0)
MIN_CHANNEL        0
MAX_CHANNEL        65535
ZERO               0UL
CHANNEL_CLAMP (x)  CLAMP (x,0,65535)
WP_CLAMP (x)       CLAMP (x,0,65535)
CHANNEL_MULT (x,y) (x * y) / 65535UL 
CHANNEL_ROUND (x)  ROUND (x)
PRINT (x)	   printf (" %uh", x); 
DATATYPE_END
