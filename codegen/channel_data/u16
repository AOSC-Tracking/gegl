DATATYPE_BEGIN
DATATYPE          guint16
WP                65535
WP_NORM           (1/65535.0)
MIN_CHANNEL       0
MAX_CHANNEL       65535
ZERO              0
CHANNEL_CLAMP (x) CLAMP(x,0,65535)
WP_CLAMP(x)       CLAMP(x,0,65535)
CHANNEL_MULT(x,y) INT_MULT16(x,y)
CHANNEL_ROUND(x)  ROUND(x)
DATATYPE_END
