DT_MACROS_BEGIN
DATATYPE		gfloat
PROMOTE_TYPE		
SIGNED_PROMOTE_TYPE	
WP			1.0
WP_NORM			1.0
CHANNEL_MIN		FLOAT_MIN
CHANNEL_MAX		FLOAT_MAX
ZERO			0.0
CHANNEL_CLAMP (x)	x
WP_CLAMP (x)		CLAMP (x,0.0,1.0)
CHANNEL_MULT (x,y)	x * y
CHANNEL_ROUND (x)	x
PRINT (x)		printf ("%.2f ", x); 
EXTERNAL_INIT (x)	x.f
DT_MACROS_END
